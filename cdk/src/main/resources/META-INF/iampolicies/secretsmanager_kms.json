{
  "Version": "2012-10-17",
  "Statement": [
	{
	  "Sid": "SmKmsDelegate",
	  "Effect": "Allow",
	  "Action": [
		"kms:Decrypt",
		"kms:Verify",
		"kms:Encrypt",
		"kms:DescribeKey"
	  ],
	  "Resource": "*",
	  "Condition": {
		"StringEquals": {
		  "kms:ViaService": "secretsmanager.us-east-1.amazonaws.com"
		}
	  }
	}
  ]
}